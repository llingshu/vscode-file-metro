{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "shell",
            "label": "npm: watch",
            "isBackground": true,
            "command": "npm run watch",
            "options": {
                "env": {
                    "PATH": "/Users/lindstnarz/.nvm/versions/node/v24.5.0/bin:${env:PATH}"
                }
            },
            "problemMatcher": {
                "owner": "typescript",
                "source": "ts",
                "applyTo": "closedDocuments",
                "fileLocation": [
                    "relative",
                    "${workspaceFolder}"
                ],
                "pattern": [
                    {
                        "regexp": "\\[tsl\\] ERROR in (.*)\\((\\d+),(\\d+)\\)",
                        "file": 1,
                        "line": 2,
                        "column": 3
                    },
                    {
                        "regexp": "\\s*TS(\\d+):\\s*(.*)$",
                        "code": 1,
                        "message": 2
                    }
                ],
                "background": {
                    "activeOnStart": true,
                    "beginsPattern": {
                        "regexp": ".*webpack --watch.*"
                    },
                    "endsPattern": {
                        "regexp": ".*compiled successfully.*"
                    }
                }
            },
            "presentation": {
                "reveal": "never"
            },
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "type": "shell",
            "label": "npm: compile",
            "command": "npm run compile",
            "options": {
                "env": {
                    "PATH": "/Users/lindstnarz/.nvm/versions/node/v24.5.0/bin:${env:PATH}"
                }
            },
            "problemMatcher": {
                "owner": "typescript",
                "source": "ts",
                "applyTo": "closedDocuments",
                "fileLocation": [
                    "relative",
                    "${workspaceFolder}"
                ],
                "pattern": [
                    {
                        "regexp": "\\[tsl\\] ERROR in (.*)\\((\\d+),(\\d+)\\)",
                        "file": 1,
                        "line": 2,
                        "column": 3
                    },
                    {
                        "regexp": "\\s*TS(\\d+):\\s*(.*)$",
                        "code": 1,
                        "message": 2
                    }
                ]
            },
            "group": "build",
            "presentation": {
                "reveal": "never"
            }
        }
    ]
}